    "# Aggregate the nodes of each title to find the span of the \n",
    "# entire title.\n",
    "titles = (\n",
    "    selected_nodes\n",
    "    .groupby(\"person\")\n",
    "    .aggregate({\"span\": \"sum\"})\n",
    "    .reset_index()\n",
    "    .rename(columns={\"span\": \"title\"})\n",
    ")\n",
    "titles"
